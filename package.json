{
  "name": "mapeo-desktop",
  "description": "An experimental offline mapping app based on iD Editor, for indigenous territory mapping in remote environments",
  "productName": "Mapeo",
  "version": "3.0.1",
  "productDescription": "Offline Map Editor",
  "author": {
    "name": "Digital Democracy",
    "email": "info@ddem.us"
  },
  "main": "index.js",
  "engine": {
    "node": "8.11.4"
  },
  "scripts": {
    "clean": "rimraf \"dist/\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --publish=onTagOrDraft",
    "build": "webpack",
    "lint": "standard",
    "test": "npm run lint && npm run test-integration",
    "test-integration": "tape test",
    "device": "rimraf \"test/test-data\" && DEBUG=* node test/mock.js",
    "watch": "webpack --watch",
    "start": "electron . --disable-http-cache",
    "server": "node index.js --headless",
    "dev": "electron . --debug --disable-http-cache",
    "postinstall": "electron-builder install-app-deps && npmpd",
    "rebuild-leveldb": "cd node_modules/leveldown && cross-env HOME=~/.electron-gyp node-gyp rebuild --target=2.0.7 --runtime=electron --arch=x64 --dist-url=https://atom.io/download/atom-shell"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digidem/mapeo-desktop.git"
  },
  "keywords": [
    "osm",
    "p2p",
    "peer-to-peer",
    "gis",
    "maps",
    "iD",
    "osm-p2p"
  ],
  "contributors": [
    "substack",
    "Gregor MacLennan <gmaclennan@digital-democracy.org>",
    "noffle",
    "karissa"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digidem/mapeo-desktop/issues"
  },
  "homepage": "https://github.com/digidem/mapeo-desktop#readme",
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^3.0.1",
    "JSONStream": "^1.3.4",
    "application-config": "^1.0.1",
    "application-config-path": "^0.1.0",
    "blob-to-buffer": "^1.2.6",
    "body": "^5.1.0",
    "concat-stream": "^1.5.1",
    "dialogs": "^1.1.20",
    "dom-helpers": "^3.3.1",
    "ecstatic": "^2.2.1",
    "electron-debug": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "end-of-stream": "^1.1.0",
    "fs-copy-file-sync": "^1.0.1",
    "glob": "^7.1.2",
    "gtran-shapefile": "^1.1.3",
    "html-react-parser": "^0.4.6",
    "hyperlog": "^4.12.0",
    "hyperlog-index": "^5.2.2",
    "hyperlog-sneakernet-replicator": "^1.1.2",
    "hyperquest": "^2.1.3",
    "id-mapeo": "^1.3.1",
    "inherits": "^2.0.3",
    "insert-css": "^1.0.0",
    "level": "^1.4.0",
    "levelup": "^1.3.1",
    "lodash": "^4.17.4",
    "mapeo-server": "^8.1.0",
    "mapeo-styles": "^1.0.0",
    "minimist": "^1.2.0",
    "mirror-folder": "^2.1.1",
    "mkdirp": "^0.5.1",
    "object-assign": "^4.1.1",
    "once": "^1.3.3",
    "osm-p2p": "^2.0.0",
    "osm-p2p-api": "^1.0.1",
    "osm-p2p-geojson": "^3.0.4",
    "osm-p2p-server": "^2.3.5",
    "prop-types": "^15.6.0",
    "pump": "^1.0.3",
    "randombytes": "^2.0.6",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-intl": "^2.4.0",
    "react-mapfilter": "^1.2.2",
    "rimraf": "^2.5.2",
    "run-series": "^1.1.4",
    "safe-fs-blob-store": "^1.0.0",
    "semver": "^5.5.1",
    "split2": "^2.0.1",
    "statuses": "^1.3.0",
    "styled-components": "^3.4.4",
    "subleveldown": "^2.1.0",
    "tar-fs": "^1.16.0",
    "through2": "^2.0.3",
    "to2": "^1.0.0",
    "traverse": "^0.6.6",
    "websocket-stream": "^3.1.0",
    "xhr": "^2.2.0",
    "xtend": "^4.0.1"
  },
  "darwinDependencies": {
    "appdmg": "0.4.5"
  },
  "devDependencies": {
    "electron-packager": "^7.6.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "spectron": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concat-stream": "^1.6.2",
    "cp-file": "^6.0.0",
    "cross-env": "^1.0.8",
    "cross-spawn": "^4.0.2",
    "devtron": "^1.2.0",
    "electron": "^2.0.7",
    "electron-builder": "^20.8.1",
    "fs-extra": "^0.30.0",
    "hubfs.js": "^1.0.0",
    "hyperquest": "^2.1.3",
    "mock-data": "^1.5.5",
    "npm-platform-dependencies": "0.0.12",
    "pngjs": "^3.3.3",
    "standard": "^12.0.1",
    "tape": "^4.9.1",
    "webpack": "^3.12.0",
    "webpack-node-externals": "^1.7.2"
    "watchify": "^3.7.0"
  },
  "build": {
    "appId": "org.digital-democracy.mapeo-desktop",
    "productName": "Mapeo",
    "artifactName": "Installar_Mapeo_v${version}_${platform}.${ext}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Utility"
    },
    "dmg": {
      "icon": "build/mapeo_installer.icns",
      "title": "{productName} v{version}",
      "contents": [
        {
          "x": 438,
          "y": 344,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 344,
          "type": "file"
        }
      ]
    },
    "files": [
      "!node_modules/osm-p2p-db/benchmark${/*}",
      "!test${/*}"
    ]
  }
}
